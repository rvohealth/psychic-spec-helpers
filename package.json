{
  "type": "module",
  "name": "@rvoh/psychic-spec-helpers",
  "version": "0.4.1",
  "description": "psychic framework spec helpers",
  "author": "RVO Health",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rvohealth/psychic-spec-helpers.git"
  },
  "main": "./dist/cjs/src/index.js",
  "module": "./dist/esm/src/index.js",
  "types": "./dist/types/src/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/src/index.js",
      "import": "./dist/esm/src/index.js",
      "types": "./dist/types/src/index.d.ts"
    }
  },
  "files": [
    "tsconfig.json",
    "src/**/*",
    "dist/**/*"
  ],
  "scripts": {
    "build": "echo \"building psychic-spec-helpers...\" && rm -rf dist && npx tsc -p ./tsconfig.json",
    "prepack": "yarn build"
  },
  "packageManager": "yarn@4.4.1",
  "peerDependencies": {
    "@types/node": "*",
    "@types/supertest": "*",
    "puppeteer": "*",
    "supertest": "*",
    "ts-node": "*",
    "typescript": "*"
  },
  "devDependencies": {
    "@types/cookiejar": "^2",
    "@types/node": "^22.5.1",
    "@types/supertest": "^6.0.2",
    "puppeteer": "^24.4.0",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "vitest": "^3.0.9"
  },
  "dependencies": {
    "cookiejar": "^2.1.4"
  }
}
